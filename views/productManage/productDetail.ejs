<h1 class="modal-title">물품 대여</h1>
<div class="modal-body">
    <p id="product_name"></p>
    <p id="product_category"></p>

    <p id="product_code"></p>
    <p id="rental_availability"></p>
    <p id="return_needed"></p>
    <p id="quantity"></p>


    <button onclick="rental()">대여자 명단</button>
    <button onclick="history()">대여 이력</button>
    <button onclick="edit()">편집</button>
    <div>
        <div id="inputStart"></div>
        <div id="inputEnd"></div>
        <div id="inputText"></div>
        <button>대여</button>
    </div>
</div>
<form action="/editProduct" method="post" id="form">
    <input type="hidden" id="hiddenCode" value="">
</form>
<script>
    function edit(){
        const form = document.getElementById('form');
        form.submit();
    }
</script>
